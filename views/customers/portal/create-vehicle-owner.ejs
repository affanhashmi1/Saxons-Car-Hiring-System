<% include ../../partials/header.ejs %>
<% include ../../partials/nav.ejs %>

<section class="my-5">
  <div class="container">
    <h3>Create Vehicle Owner</h3>
    <hr />
    <div class="row">
      <div class="col-md-6">
        <div class="form-group">
          <label>Name</label>
          <input type="text" name="create-vehicle-owner-name" id="create-vehicle-owner-name"
            class="form-control form-control-sm" />
        </div>
        <div class="form-group">
          <label>CNIC</label>
          <input type="text" name="create-vehicle-owner-cnic" id="create-vehicle-owner-cnic"
            class="form-control form-control-sm" />
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label>Contact Number</label>
          <input type="text" name="create-vehicle-owner-contact-number" id="create-vehicle-owner-contact-number"
            class="form-control form-control-sm" />
        </div>
        <div class="form-group">
          <label>Address</label>
          <textarea name="create-vehicle-owner-address" id="create-vehicle-owner-address"
            class="form-control form-control-sm" rows="6"></textarea>
        </div>
      </div>
    </div>
    <hr />
    <div class="clearfix">
      <button type="button" name="create-vehicle-owner-btn" id="create-vehicle-owner-btn"
        class="btn btn-primary float-right">Create</button>
    </div>
  </div>
</section>

<% include ../../partials/footer.ejs %>